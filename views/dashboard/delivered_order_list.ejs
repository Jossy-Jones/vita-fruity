<script src="http://rawgithub.com/stidges/jquery-searchable/master/dist/jquery.searchable-1.0.0.min.js"></script>

<script type="text/javascript">
var noResultAlert = $("#noResult");
var noResultAlertSearchTerm = $("#noResult #searchTerm");

$(function () {
    $( '#OrdersTable' ).searchable({
        searchField: '#search',
        striped: true,
        oddRow: { 'background-color': '#f5f5f5' },
        evenRow: { 'background-color': '#fff' },
        searchType: 'fuzzy',
        onSearchActive : function( noResultAlert , noResultAlertSearchTerm ) {
            noResultAlert.hide();
        },
        onSearchEmpty: function( noResultAlert ) {
            noResultAlert.show();
        }        
    });
    
}); 
</script>

